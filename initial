import time

# Initialize a variable to store the sum
total = 0

# Loop through numbers from 1 to 200
for num in range(1, 201):
    total += num

    # Check if the current number is 100
    if num == 100:
        print("Reached 100. Pausing for 5 seconds...")
        time.sleep(5)  # Pause for 5 seconds

# Print the final sum
print("The sum of numbers from 1 to 200 is:", total)
